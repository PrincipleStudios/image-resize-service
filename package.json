{
  "name": "image-resize-service",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/functions/*.js",
  "scripts": {
    "dev": "nodemon",
    "predev:run": "npm run clean && npm run build",
    "dev:run": "func start",
    "start": "func start",
    "clean": "rimraf dist",
    "prebuild": "openapi-codegen-typescript schemas/api.yaml src/api-types/ -c",
    "build": "swc src -d dist --source-maps --copy-files",
    "lint": "npm run typecheck && npm run eslint && npm run prettier",
    "lint:fix": "npm run typecheck && npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint \"**/*.{cjs,mjs,js,ts,tsx}\"",
    "eslint:fix": "eslint \"**/*.{cjs,mjs,js,ts,tsx}\" --fix",
    "prettier": "prettier --list-different \"src/**/*.{cjs,mjs,js,ts,tsx,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{cjs,mjs,js,ts,tsx,md}\"",
    "typecheck": "tsc --noemit",
    "prepare": "husky install .husky || echo 0"
  },
  "author": "Principle Studios",
  "license": "ISC",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/functions": "^4.0.0-alpha.9",
    "@azure/storage-blob": "^12.14.0",
    "@principlestudios/openapi-codegen-typescript": "^0.6.0",
    "dotenv": "^16.1.4",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.1",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.63",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "azure-functions-core-tools": "^4.0.5198",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
